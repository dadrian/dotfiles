{
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.suggestSelection": "first",
    "editor.lineNumbers": "off",
    "editor.cursorStyle": "block",
    "editor.autoClosingOvertype": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoClosingDelete": "never",
    "editor.autoClosingBrackets": "never",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.colorTheme": "dadrian",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "[go]": {
        "editor.insertSpaces": false,
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "formatTool": "goimports",
        "lintTool": "golangci-lint",
        "testTimeout": "10s",
        "useLanguageServer": true
    },
    "[go.tmpl]": {
        "editor.insertSpaces": false
    },
    "[html]": {
        "autoClosingTags": false,
        "editor.tabSize": 2
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "[typescript]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[javascript]": {
        "autoClosingTags": false,
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[typescriptreact]": {
        "autoClosingTags": false,
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[zig]":{
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.inlayHints.enabled": "offUnlessPressed"
    },
    "editor.fontFamily": "Menlo, Fira Code, Monaco, Consolas, 'Courier New', monospace",
    "zig.path": "",
    "zig.zls.path": "/Users/dadrian/Library/Application Support/Code/User/globalStorage/ziglang.vscode-zig/zls_install/zls",
    "zig.initialSetupDone": true,
    "cmake.showOptionsMovedNotification": false,
    "cmake.configureOnOpen": false,
    "python.venvPath": "env",
    "cSpell.enabled": false
}
